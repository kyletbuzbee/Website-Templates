{
  "version": "1.0.0",
  "template": "creative",
  "description": "CMS field mapping for Creative template - portfolios, agencies, and creative professionals",
  "cms_mappings": {
    "wordpress": {
      "name": "WordPress",
      "acf_version": ">=5.0",
      "field_groups": [
        {
          "title": "Company Information",
          "location": [
            {
              "param": "page_template",
              "operator": "==",
              "value": "page-home.php"
            }
          ],
          "fields": [
            {
              "key": "company_name",
              "label": "Company Name",
              "name": "company_name",
              "type": "text",
              "required": 1,
              "default_value": "Your Creative Studio"
            },
            {
              "key": "company_tagline",
              "label": "Company Tagline",
              "name": "company_tagline",
              "type": "text",
              "default_value": "Where Creativity Meets Strategy"
            },
            {
              "key": "company_description",
              "label": "Company Description",
              "name": "company_description",
              "type": "textarea",
              "rows": 4,
              "required": 1
            },
            {
              "key": "phone",
              "label": "Phone Number",
              "name": "phone",
              "type": "text",
              "required": 1
            },
            {
              "key": "email",
              "label": "Email Address",
              "name": "email",
              "type": "email",
              "required": 1
            },
            {
              "key": "address",
              "label": "Business Address",
              "name": "address",
              "type": "textarea",
              "rows": 3
            },
            {
              "key": "year_established",
              "label": "Year Established",
              "name": "year_established",
              "type": "text"
            }
          ]
        },
        {
          "title": "Branding",
          "fields": [
            {
              "key": "logo",
              "label": "Company Logo",
              "name": "logo",
              "type": "image",
              "return_format": "url"
            },
            {
              "key": "favicon",
              "label": "Favicon",
              "name": "favicon",
              "type": "image",
              "return_format": "url"
            },
            {
              "key": "primary_color",
              "label": "Primary Color",
              "name": "primary_color",
              "type": "color_picker",
              "default_value": "#6366f1"
            },
            {
              "key": "secondary_color",
              "label": "Secondary Color",
              "name": "secondary_color",
              "type": "color_picker",
              "default_value": "#ec4899"
            }
          ]
        },
        {
          "title": "Social Media",
          "fields": [
            {
              "key": "instagram_url",
              "label": "Instagram URL",
              "name": "instagram_url",
              "type": "url"
            },
            {
              "key": "dribbble_url",
              "label": "Dribbble URL",
              "name": "dribbble_url",
              "type": "url"
            },
            {
              "key": "behance_url",
              "label": "Behance URL",
              "name": "behance_url",
              "type": "url"
            },
            {
              "key": "linkedin_url",
              "label": "LinkedIn URL",
              "name": "linkedin_url",
              "type": "url"
            }
          ]
        },
        {
          "title": "Hero Section",
          "fields": [
            {
              "key": "hero_title",
              "label": "Hero Title",
              "name": "hero_title",
              "type": "text",
              "required": 1,
              "default_value": "Crafting Digital Experiences That Matter"
            },
            {
              "key": "hero_subtitle",
              "label": "Hero Subtitle",
              "name": "hero_subtitle",
              "type": "textarea",
              "rows": 3,
              "required": 1
            },
            {
              "key": "hero_image",
              "label": "Hero Image",
              "name": "hero_image",
              "type": "image",
              "required": 1,
              "return_format": "url"
            },
            {
              "key": "hero_primary_cta",
              "label": "Primary CTA Text",
              "name": "hero_primary_cta",
              "type": "text",
              "default_value": "View Our Work"
            },
            {
              "key": "hero_secondary_cta",
              "label": "Secondary CTA Text",
              "name": "hero_secondary_cta",
              "type": "text",
              "default_value": "Start a Project"
            },
            {
              "key": "hero_scroll_text",
              "label": "Scroll Indicator Text",
              "name": "hero_scroll_text",
              "type": "text",
              "default_value": "Discover our process"
            }
          ]
        },
        {
          "title": "Work/Portfolio Section",
          "fields": [
            {
              "key": "work_title",
              "label": "Work Section Title",
              "name": "work_title",
              "type": "text",
              "default_value": "Featured Work"
            },
            {
              "key": "work_subtitle",
              "label": "Work Section Subtitle",
              "name": "work_subtitle",
              "type": "textarea",
              "rows": 2
            },
            {
              "key": "work_categories",
              "label": "Work Categories",
              "name": "work_categories",
              "type": "repeater",
              "sub_fields": [
                {
                  "key": "category_name",
                  "label": "Category Name",
                  "name": "name",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "category_slug",
                  "label": "Category Slug",
                  "name": "slug",
                  "type": "text",
                  "required": 1
                }
              ]
            },
            {
              "key": "work_items",
              "label": "Work Items",
              "name": "work_items",
              "type": "repeater",
              "min": 6,
              "layout": "block",
              "sub_fields": [
                {
                  "key": "work_title",
                  "label": "Project Title",
                  "name": "title",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "work_image",
                  "label": "Project Image",
                  "name": "image",
                  "type": "image",
                  "required": 1,
                  "return_format": "url"
                },
                {
                  "key": "work_category",
                  "label": "Project Category",
                  "name": "category",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "work_category_name",
                  "label": "Category Display Name",
                  "name": "category_name",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "work_link",
                  "label": "Project Link",
                  "name": "link",
                  "type": "url"
                }
              ]
            }
          ]
        },
        {
          "title": "About Section",
          "fields": [
            {
              "key": "about_title",
              "label": "About Section Title",
              "name": "about_title",
              "type": "text",
              "default_value": "About Our Studio"
            },
            {
              "key": "about_description",
              "label": "About Description",
              "name": "about_description",
              "type": "textarea",
              "rows": 4,
              "required": 1
            },
            {
              "key": "about_image",
              "label": "About Image",
              "name": "about_image",
              "type": "image",
              "return_format": "url"
            },
            {
              "key": "about_stats",
              "label": "About Statistics",
              "name": "about_stats",
              "type": "repeater",
              "sub_fields": [
                {
                  "key": "stat_number",
                  "label": "Statistic Number",
                  "name": "number",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "stat_label",
                  "label": "Statistic Label",
                  "name": "label",
                  "type": "text",
                  "required": 1
                }
              ]
            },
            {
              "key": "about_skills",
              "label": "Skills/Expertise",
              "name": "about_skills",
              "type": "repeater",
              "sub_fields": [
                {
                  "key": "skill_name",
                  "label": "Skill Name",
                  "name": "name",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "skill_percentage",
                  "label": "Proficiency Percentage",
                  "name": "percentage",
                  "type": "number",
                  "min": 0,
                  "max": 100,
                  "required": 1
                }
              ]
            }
          ]
        },
        {
          "title": "Services Section",
          "fields": [
            {
              "key": "services_title",
              "label": "Services Section Title",
              "name": "services_title",
              "type": "text",
              "default_value": "Our Services"
            },
            {
              "key": "services_subtitle",
              "label": "Services Section Subtitle",
              "name": "services_subtitle",
              "type": "textarea",
              "rows": 2
            },
            {
              "key": "services",
              "label": "Services",
              "name": "services",
              "type": "repeater",
              "required": 1,
              "min": 3,
              "layout": "block",
              "sub_fields": [
                {
                  "key": "service_title",
                  "label": "Service Title",
                  "name": "title",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "service_description",
                  "label": "Service Description",
                  "name": "description",
                  "type": "textarea",
                  "rows": 3,
                  "required": 1
                },
                {
                  "key": "service_icon",
                  "label": "Service Icon",
                  "name": "icon",
                  "type": "image",
                  "return_format": "url"
                },
                {
                  "key": "service_link",
                  "label": "Service Page Link",
                  "name": "link",
                  "type": "url"
                },
                {
                  "key": "service_features",
                  "label": "Service Features",
                  "name": "features",
                  "type": "repeater",
                  "sub_fields": [
                    {
                      "key": "feature_text",
                      "label": "Feature",
                      "name": "feature",
                      "type": "text"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "title": "Testimonials Section",
          "fields": [
            {
              "key": "testimonials_title",
              "label": "Testimonials Section Title",
              "name": "testimonials_title",
              "type": "text",
              "default_value": "Client Love"
            },
            {
              "key": "testimonials_subtitle",
              "label": "Testimonials Subtitle",
              "name": "testimonials_subtitle",
              "type": "textarea",
              "rows": 2
            },
            {
              "key": "testimonials",
              "label": "Testimonials",
              "name": "testimonials",
              "type": "repeater",
              "min": 3,
              "layout": "block",
              "sub_fields": [
                {
                  "key": "testimonial_name",
                  "label": "Client Name",
                  "name": "name",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "testimonial_company",
                  "label": "Client Company",
                  "name": "company",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "testimonial_avatar",
                  "label": "Client Avatar",
                  "name": "avatar",
                  "type": "image",
                  "return_format": "url"
                },
                {
                  "key": "testimonial_quote",
                  "label": "Testimonial Quote",
                  "name": "quote",
                  "type": "textarea",
                  "rows": 4,
                  "required": 1
                }
              ]
            }
          ]
        },
        {
          "title": "Process Section",
          "fields": [
            {
              "key": "process_title",
              "label": "Process Section Title",
              "name": "process_title",
              "type": "text",
              "default_value": "Our Process"
            },
            {
              "key": "process_subtitle",
              "label": "Process Subtitle",
              "name": "process_subtitle",
              "type": "textarea",
              "rows": 2
            },
            {
              "key": "process_steps",
              "label": "Process Steps",
              "name": "process_steps",
              "type": "repeater",
              "min": 4,
              "layout": "block",
              "sub_fields": [
                {
                  "key": "step_number",
                  "label": "Step Number",
                  "name": "number",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "step_title",
                  "label": "Step Title",
                  "name": "title",
                  "type": "text",
                  "required": 1
                },
                {
                  "key": "step_description",
                  "label": "Step Description",
                  "name": "description",
                  "type": "textarea",
                  "rows": 3,
                  "required": 1
                },
                {
                  "key": "step_icon",
                  "label": "Step Icon",
                  "name": "icon",
                  "type": "image",
                  "return_format": "url"
                }
              ]
            }
          ]
        }
      ]
    },
    "contentful": {
      "name": "Contentful",
      "content_types": [
        {
          "name": "Company Info",
          "id": "companyInfo",
          "fields": [
            {
              "name": "Company Name",
              "id": "companyName",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Tagline",
              "id": "tagline",
              "type": "Symbol"
            },
            {
              "name": "Description",
              "id": "description",
              "type": "Text",
              "required": true
            },
            {
              "name": "Phone",
              "id": "phone",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Email",
              "id": "email",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Address",
              "id": "address",
              "type": "Text"
            },
            {
              "name": "Year Established",
              "id": "yearEstablished",
              "type": "Symbol"
            }
          ]
        },
        {
          "name": "Work Item",
          "id": "workItem",
          "fields": [
            {
              "name": "Title",
              "id": "title",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Image",
              "id": "image",
              "type": "Link",
              "linkType": "Asset",
              "required": true
            },
            {
              "name": "Category",
              "id": "category",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Category Name",
              "id": "categoryName",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Link",
              "id": "link",
              "type": "Symbol"
            }
          ]
        },
        {
          "name": "Service",
          "id": "service",
          "fields": [
            {
              "name": "Title",
              "id": "title",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Description",
              "id": "description",
              "type": "Text",
              "required": true
            },
            {
              "name": "Icon",
              "id": "icon",
              "type": "Link",
              "linkType": "Asset"
            },
            {
              "name": "Link",
              "id": "link",
              "type": "Symbol"
            },
            {
              "name": "Features",
              "id": "features",
              "type": "Array",
              "items": {
                "type": "Symbol"
              }
            }
          ]
        },
        {
          "name": "Testimonial",
          "id": "testimonial",
          "fields": [
            {
              "name": "Name",
              "id": "name",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Company",
              "id": "company",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Avatar",
              "id": "avatar",
              "type": "Link",
              "linkType": "Asset"
            },
            {
              "name": "Quote",
              "id": "quote",
              "type": "Text",
              "required": true
            }
          ]
        },
        {
          "name": "Process Step",
          "id": "processStep",
          "fields": [
            {
              "name": "Number",
              "id": "number",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Title",
              "id": "title",
              "type": "Symbol",
              "required": true
            },
            {
              "name": "Description",
              "id": "description",
              "type": "Text",
              "required": true
            },
            {
              "name": "Icon",
              "id": "icon",
              "type": "Link",
              "linkType": "Asset"
            }
          ]
        }
      ]
    },
    "strapi": {
      "name": "Strapi",
      "content_types": [
        {
          "name": "company-info",
          "attributes": {
            "companyName": {
              "type": "string",
              "required": true
            },
            "tagline": {
              "type": "string"
            },
            "description": {
              "type": "text",
              "required": true
            },
            "phone": {
              "type": "string",
              "required": true
            },
            "email": {
              "type": "email",
              "required": true
            },
            "address": {
              "type": "text"
            },
            "yearEstablished": {
              "type": "string"
            }
          }
        },
        {
          "name": "work-item",
          "attributes": {
            "title": {
              "type": "string",
              "required": true
            },
            "image": {
              "type": "media",
              "multiple": false,
              "required": true
            },
            "category": {
              "type": "string",
              "required": true
            },
            "categoryName": {
              "type": "string",
              "required": true
            },
            "link": {
              "type": "string"
            }
          }
        },
        {
          "name": "service",
          "attributes": {
            "title": {
              "type": "string",
              "required": true
            },
            "description": {
              "type": "text",
              "required": true
            },
            "icon": {
              "type": "media",
              "multiple": false
            },
            "link": {
              "type": "string"
            },
            "features": {
              "type": "json"
            }
          }
        },
        {
          "name": "testimonial",
          "attributes": {
            "name": {
              "type": "string",
              "required": true
            },
            "company": {
              "type": "string",
              "required": true
            },
            "avatar": {
              "type": "media",
              "multiple": false
            },
            "quote": {
              "type": "text",
              "required": true
            }
          }
        },
        {
          "name": "process-step",
          "attributes": {
            "number": {
              "type": "string",
              "required": true
            },
            "title": {
              "type": "string",
              "required": true
            },
            "description": {
              "type": "text",
              "required": true
            },
            "icon": {
              "type": "media",
              "multiple": false
            }
          }
        }
      ]
    },
    "webflow": {
      "name": "Webflow",
      "collections": [
        {
          "name": "Company Info",
          "fields": [
            {
              "name": "Company Name",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Tagline",
              "type": "Plain text"
            },
            {
              "name": "Description",
              "type": "Multi-line text",
              "required": true
            },
            {
              "name": "Phone",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Email",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Address",
              "type": "Multi-line text"
            },
            {
              "name": "Year Established",
              "type": "Plain text"
            }
          ]
        },
        {
          "name": "Work Items",
          "fields": [
            {
              "name": "Project Title",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Project Image",
              "type": "Image",
              "required": true
            },
            {
              "name": "Category",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Category Display Name",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Project Link",
              "type": "Link"
            }
          ]
        },
        {
          "name": "Services",
          "fields": [
            {
              "name": "Service Title",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Description",
              "type": "Multi-line text",
              "required": true
            },
            {
              "name": "Service Icon",
              "type": "Image"
            },
            {
              "name": "Service Link",
              "type": "Link"
            },
            {
              "name": "Service Features",
              "type": "Multi-line text"
            }
          ]
        },
        {
          "name": "Testimonials",
          "fields": [
            {
              "name": "Client Name",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Client Company",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Client Avatar",
              "type": "Image"
            },
            {
              "name": "Testimonial Quote",
              "type": "Multi-line text",
              "required": true
            }
          ]
        },
        {
          "name": "Process Steps",
          "fields": [
            {
              "name": "Step Number",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Step Title",
              "type": "Plain text",
              "required": true
            },
            {
              "name": "Step Description",
              "type": "Multi-line text",
              "required": true
            },
            {
              "name": "Step Icon",
              "type": "Image"
            }
          ]
        }
      ]
    }
  },
  "field_mapping_guide": {
    "template_variables": {
      "company_name": "Company/companyName/company_name",
      "hero_title": "Hero/hero_title/heroTitle",
      "work_items": "Work/work_items/workItems (array)",
      "services": "Services/services/services (array)",
      "testimonials": "Testimonials/testimonials/testimonials (array)",
      "process_steps": "Process/process_steps/processSteps (array)"
    },
    "data_transformation": {
      "work_categories": "Map work items to category arrays",
      "services_array": "Map each service object to template service structure with features array",
      "image_urls": "Convert CMS image references to full URLs",
      "social_links": "Convert social media fields to social object"
    }
  },
  "implementation_notes": {
    "wordpress": "Use ACF plugin for advanced custom fields. Create field groups as shown above.",
    "contentful": "Create content types in Contentful dashboard. Use the GraphQL API for data fetching.",
    "strapi": "Create content types in Strapi admin panel. Use REST or GraphQL API.",
    "webflow": "Create collections in Webflow designer. Use Webflow's CMS API for dynamic content."
  }
}
