{
  "name": "production-ready-templates",
  "version": "1.0.0",
  "description": "Production-ready website templates with automated asset distribution and modern build system",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run assets:build && tsc && vite build",
    "build:full": "npm run assets:build && npm run update-templates && tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lhci autorun --config=lighthouserc.json",
    "pwa:build": "vite build",
    "pwa:preview": "vite preview",
    "accessibility": "node scripts/accessibility-ci.js",
    "accessibility:full": "node scripts/accessibility-test.js",
    "assets:build": "npm run assets:distribute && npm run assets:optimize",
    "assets:distribute": "node scripts/distribute-assets.js",
    "assets:optimize": "node scripts/optimize-assets.js",
    "assets:rename-icons": "node scripts/distribute-assets.js --rename-icons",
    "bulk-download": "node scripts/bulk-image-downloader.js",
    "distribute-assets": "node scripts/distribute-assets.js",
    "optimize-assets": "node scripts/optimize-assets.js",
    "update-templates": "node scripts/update-templates.js",
    "update-templates-with-icons": "node scripts/update-templates-with-icons.js",
    "optimize-gallery-images": "node scripts/optimize-gallery-images.js",
    "optimize-images-standardized": "node scripts/optimize-images-standardized.js",
    "update-templates-with-standardized-images": "node scripts/update-templates-with-standardized-images.js",
    "audit-template-assets": "node scripts/audit-template-assets.js",
    "generate-missing-assets": "node scripts/generate-missing-assets.js",
    "test-pipeline": "node scripts/test-pipeline.js",
    "deploy:prepare": "npm run build:full && npm run accessibility && npm run lighthouse:local"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^6.0.4",
    "@egjs/infinitegrid": "^4.12.0",
    "@fancyapps/ui": "^6.1.5",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/material-icons": "^5.2.7",
    "@fontsource/material-icons-outlined": "^5.2.6",
    "@fontsource/poppins": "^5.2.7",
    "@fontsource/roboto-mono": "^5.2.8",
    "@material-design-icons/font": "^0.14.15",
    "@material-design-icons/svg": "^0.14.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@splidejs/splide": "^4.1.4",
    "@squoosh/lib": "^0.3.1",
    "@tabler/icons-react": "^3.35.0",
    "baguettebox.js": "^1.12.0",
    "bootstrap-icons": "^1.13.1",
    "choco": "^0.2.1",
    "fancybox": "^3.0.1",
    "flickr-sdk": "^7.1.0",
    "fs-extra": "^11.2.0",
    "gallery-dl": "^1.0.2",
    "git": "^0.1.5",
    "glob": "^10.3.10",
    "gsap": "^3.12.5",
    "heroicons": "^2.2.0",
    "imagemin": "^9.0.1",
    "ionicons": "^8.0.13",
    "isotope-packery": "^2.0.1",
    "justified-gallery": "^0.1.0",
    "lucide-react": "^0.553.0",
    "magnific-popup": "^1.2.0",
    "masonry-layout": "^4.2.2",
    "pexels": "^1.4.0",
    "phosphor-react": "^1.4.1",
    "photoswipe": "^5.4.4",
    "pixabay-api": "^1.0.4",
    "react-icons": "^5.5.0",
    "sharp": "^0.33.3",
    "slick-carousel": "^1.8.1",
    "tiny-slider": "^2.9.4",
    "unsplash-js": "^7.0.20",
    "viewerjs": "^1.11.7",
    "wget": "^0.0.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@lhci/cli": "^0.15.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.0",
    "@types/potrace": "^2.1.5",
    "@types/swiper": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitest/ui": "^1.6.0",
    "axe-core": "^4.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "install": "^0.13.0",
    "jsdom": "^24.1.0",
    "npm": "^11.6.2",
    "p-limit": "^7.2.0",
    "postcss": "^8.4.38",
    "postcss-preset-env": "^9.5.14",
    "potrace": "^2.1.8",
    "prettier": "^3.3.3",
    "puppeteer": "^24.29.1",
    "swiper": "^12.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^5.3.4",
    "vite-plugin-pwa": "^0.20.1",
    "vitest": "^1.6.0",
    "workbox-build": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "keywords": [
    "templates",
    "website",
    "automation",
    "assets",
    "vite",
    "pwa",
    "modern-web"
  ],
  "author": "",
  "license": "ISC"
}
